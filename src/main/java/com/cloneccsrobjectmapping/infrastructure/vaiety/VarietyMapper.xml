<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cloneccsrobjectmapping.infrastructure.vaiety.VarietyMapper">
    <select id="allVarietiesList" resultType="com.cloneccsrobjectmapping.domain.model.variety.Variety">
        SELECT
            品種.品種番号 AS "varietyNumber.value"
            , 品種.名前 AS "varietyName.value"
        FROM
            商品.品種 品種
    </select>
</mapper>